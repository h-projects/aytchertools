{
  "name": "aytchertools",
  "version": "0.1.0",
  "description": "Aytch Software's H tools",
  "scripts": {
    "test": "vitest run",
    "build": "tsup",
    "check": "biome check .",
    "check:apply": "biome check --apply .",
    "format": "biome format --write .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit"
  },
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    "import": "./dist/index.mjs",
    "require": "./dist/index.js",
    "types": "./dist/index.d.ts"
  },
  "files": [
    "dist"
  ],
  "author": "Aytch Software",
  "license": "ISC",
  "keywords": [
    "aytch-software",
    "h"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/h-projects/aytchertools.git"
  },
  "bugs": {
    "url": "https://github.com/h-projects/aytchertools/issues"
  },
  "homepage": "https://github.com/h-projects/aytchertools#readme",
  "devDependencies": {
    "@biomejs/biome": "^1.6.2",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "tsup": "^8.0.2",
    "typescript": "^5.4.5",
    "vitest": "^1.4.0"
  },
  "engines": {
    "node": ">=15.0.0"
  },
  "publishConfig": {
    "access": "public",
    "provenance": true
  },
  "packageManager": "pnpm@8.15.5"
}
